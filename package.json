{
  "name": "goc-spending",
  "version": "1.0.0",
  "description": "Collection of datasets about Government of Canada spending",
  "scripts": {
    "scrape": "npm run corporations-scrape",
    "upload": "npm run corporations-upload",
    "metadata": "npm run corporations-metadata && npm run corporations-json2csv",
    "corporations-scrape": "node scrapers/corporation-scraper.js",
    "corporations-metadata": "node scrapers/corporation-metadata.js",
    "corporations-json2csv": "json2csv -i corporations.json > corporations.csv",
    "corporations-upload": "aws s3 cp corporations.csv s3://goc-spending/corporations.csv && aws s3 cp corporations.json s3://goc-spending/corporations.json",
    "contracts-scrape": "php scrapers/contracts-scraper.php",
    "contracts-metadata-pwgsc": "node scrapers/contracts-metadata-pwgsc.js",
    "contracts-json2csv-pwgsc": "json2csv -i contracts-pwgsc.json > contracts-pwgsc.csv"
  },
  "author": "Ottawa CivicTech",
  "license": "MIT",
  "dependencies": {
    "cheerio": "^0.22.0",
    "d3-queue": "^3.0.3",
    "json2csv": "^3.7.3",
    "json2csv-stream": "^0.1.3",
    "load-json-file": "^2.0.0",
    "lodash": "^4.17.4",
    "progress": "^1.1.8",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "unidecode": "^0.1.8",
    "write-json-file": "^2.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.17.31",
    "@types/node": "^7.0.5",
    "typescript": "^2.1.6"
  }
}
